<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
						http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd
						">
	<!-- ***First bean*** -->
	<bean class="com.springcore.Student" name="student1" p:studentId="1011"
		p:studentName="Parth Kathiriya" p:studentAddress="Surat" />


	<!-- ***second bean*** -->
	<bean class="com.springcore.Student" name="student2" p:studentId="1012"
		p:studentName="Demo Data" p:studentAddress="Delhi" />



</beans>